{
  "name": "feriwala-backend",
  "version": "1.0.0",
  "description": "Backend API for Feriwala e-commerce platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "dev:ws": "nodemon -r tsconfig-paths/register src/websocket/server.ts",
    "build": "npx prisma generate && tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "start:ws": "node dist/websocket/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:prod": "tsx prisma/seed.production.ts",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "test:s3": "tsx scripts/test-s3.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/lib-storage": "^3.913.0",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.4",
    "nodemailer": "^6.10.1",
    "razorpay": "^2.9.2",
    "redis": "^4.6.12",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.4",
    "stripe": "^14.14.0",
    "twilio": "^4.19.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/controllers": "dist/controllers",
    "@/middleware": "dist/middleware",
    "@/models": "dist/models",
    "@/routes": "dist/routes",
    "@/services": "dist/services",
    "@/utils": "dist/utils",
    "@/types": "dist/types"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.24",
    "@types/nodemailer": "^6.4.21",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prisma": "^6.16.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
